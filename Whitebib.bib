@article{Tulloch2013,
  title = {Accounting for {{Complementarity}} to {{Maximize Monitoring Power}} for {{Species Management}}: {{Complementary Monitoring Indicators}}},
  shorttitle = {Accounting for {{Complementarity}} to {{Maximize Monitoring Power}} for {{Species Management}}},
  author = {Tulloch, Ayesha I. T. and Chad{\`e}s, Iadine and Possingham, Hugh P.},
  year = {2013},
