@incollection{Melian2014,
  title = {Individual Trait Variation and Diversity in Food Webs},
  booktitle = {Advances in {{Ecological Research}}},
  author = {Meli{\'a}n, Carlos J. and Bald{\'o}, Francisco and Matthews, Blake and Vilas, C{\'e}sar and {Gonz{\'a}lez-Orteg{\'o}n}, Enrique and Drake, Pilar and Williams, Richard J.},
